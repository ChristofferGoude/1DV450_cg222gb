<div class="row">
	<div class="large-6 columns">
		<p><b>USERNAME</b></p>
	</div>
	<div class="large-6 columns">
		<p><b>PASSWORD</b></p>
	</div>
</div>
<% @users.each do |user| %>
	<div class="row">
		<div class="large-6 columns">
			<p><%= user.email %></p>
		</div>
		<div class="large-6 columns">
			<p><%= user.password %></p>
		</div>
	</div>
<% end %>
<div class="row">
	<div class="large-12 columns">
		<%= link_to 'Log Out', logout_path, {:class => "button secondary expand"} %>
	</div>
</div>